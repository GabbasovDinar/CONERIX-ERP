<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<template id="website_favicon_bd" inherit_id="web.layout">
       		<!-- <xpath expr="//title" position="replace"></xpath>-->
       		<xpath expr="//meta[@http-equiv='content-type']" position="after">
       			<link rel="shortcut icon" href="/ws_favicon_bd/static/src/img/favicon.ico" type="image/x-icon"/>
       		</xpath>
    	</template>
    	<template id="website_favicon_ws_bd" inherit_id="website.layout">
       		<xpath expr="//meta[@name='keywords']" position="after">
       			<link rel="shortcut icon" href="/ws_favicon_bd/static/src/img/favicon.ico" type="image/x-icon"/>
       		</xpath>
    	</template>
	</data>
</openerp>
